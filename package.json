{
  "name": "portablegit",
  "version": "100.3.1+2.43.0.windows.1",
  "description": "ðŸ”„ Redistribution of Git for Windows PortableGit",
  "keywords": [
    "git",
    "npm",
    "binary",
    "portable",
    "portableapps",
    "npx",
    "redistribution",
    "git-for-windows",
    "portablegit"
  ],
  "homepage": "https://github.com/jcbhmr/portablegit.js#readme",
  "repository": "github:jcbhmr/portablegit",
  "license": "GPL-2.0-or-later",
  "contributors": [
    "Jacob Hummer <jcbhmr@outlook.com> (https://jcbhmr.me/)"
  ],
  "type": "module",
  "exports": {
    "./*": {
      "node": "./out/*"
    }
  },
  "bin": {
    "bash": "out/bin/bash.exe",
    "git": "out/bin/git.exe",
    "git-bash": "out/git-bash.exe",
    "git-cmd": "out/git-cmd.exe",
    "git-gui": "out/cmd/git-gui.exe",
    "gitk": "out/cmd/gitk.exe",
    "sh": "out/bin/sh.exe"
  },
  "files": [
    "index.js",
    "out"
  ],
  "scripts": {
    "generate": "node run.js generate",
    "build": "node run.js build",
    "pretest": "npm run build",
    "test": "npx -p . git --version && npx -p . bash --version",
    "format": "prettier -w ."
  },
  "os": [
    "win32"
  ],
  "cpu": [
    "x64"
  ],
  "devDependencies": {
    "execa": "^8.0.1",
    "octokit": "^3.2.0",
    "prettier": "^3.2.5"
  }
}
